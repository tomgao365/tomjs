{
  "name": "@tomjs/eslint",
  "version": "1.5.1",
  "description": "eslint config for tomjs",
  "keywords": [
    "eslint",
    "lint",
    "prettier",
    "config",
    "typescript",
    "vue",
    "react",
    "hooks"
  ],
  "author": {
    "name": "Tom Gao",
    "email": "tom@tomgao.cc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjs/config.git",
    "directory": "packages/eslint"
  },
  "homepage": "https://github.com/tomjs/config/tree/main/packages/commitlint#readme",
  "files": [
    "es",
    "lib",
    "*.md"
  ],
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js",
      "types": "./es/index.d.ts"
    },
    "./typescript": {
      "require": "./lib/typescript.js",
      "import": "./es/typescript.js",
      "types": "./es/typescript.d.ts"
    },
    "./vue": {
      "require": "./lib/vue/index.js",
      "import": "./es/vue/index.js",
      "types": "./es/vue/index.d.ts"
    },
    "./vue/typescript": {
      "require": "./lib/vue/typescript.js",
      "import": "./es/vue/typescript.js",
      "types": "./es/vue/typescript.d.ts"
    },
    "./vue2": {
      "require": "./lib/vue2/index.js",
      "import": "./es/vue2/index.js",
      "types": "./es/vue2/index.d.ts"
    },
    "./vue2/typescript": {
      "require": "./lib/vue2/typescript.js",
      "import": "./es/vue2/typescript.js",
      "types": "./es/vue2/typescript.d.ts"
    },
    "./react": {
      "require": "./lib/react/index.js",
      "import": "./es/react/index.js",
      "types": "./es/react/index.d.ts"
    },
    "./react/typescript": {
      "require": "./lib/react/typescript.js",
      "import": "./es/react/typescript.js",
      "types": "./es/react/typescript.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "rimraf ./es ./lib"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=3.0.0",
    "react": ">=17",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": "^16.0.0 || >=18.0.0"
  }
}
