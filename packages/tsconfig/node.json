{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "node config",
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "esnext",
    "target": "es2022",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "noImplicitAny": false
  }
}
