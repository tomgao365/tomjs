{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./vue-dom.json",
  "display": "react dom config",
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false
  }
}
