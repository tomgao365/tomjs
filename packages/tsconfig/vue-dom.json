{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@vue/tsconfig/tsconfig.json",
  "display": "vue dom config",
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false
  }
}
